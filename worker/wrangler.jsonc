{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "wedding-bells",
	"main": "src/index.ts",
	"compatibility_date": "2025-02-24",
	"observability": {
		"enabled": true
	},
	"r2_buckets": [
		{
			"binding": "AUDIO_BUCKET",
			"bucket_name": "marc-with-a-sea",
			"preview_bucket_name": "marc-with-a-sea-dev"
		}
	],
	"vars": {
		"ALLOWED_ORIGIN": "https://marc-with-a-sea.axolotl.club",
		"PUBLIC_URL": "https://marc-with-a-sea.axolotl.club"
	}
}